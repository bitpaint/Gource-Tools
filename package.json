{
  "name": "gource-tools",
  "version": "1.0.0",
  "description": "Application moderne pour visualiser l'historique Git Ã  travers Gource",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:client start:server",
    "dev": "npm-run-all --parallel dev:client dev:server",
    "start:client": "cd client && npm start",
    "start:server": "cd server && npm start",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install && cd ../electron && npm install",
    "electron": "cd electron && npm start",
    "electron:build": "cd electron && npm run build",
    "docker:build": "docker build -t gource-tools .",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/gource-tools.git"
  },
  "keywords": [
    "gource",
    "git",
    "visualization",
    "electron",
    "react",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/multer": "^1.4.12",
    "@types/node-fetch": "^2.6.12",
    "concurrently": "^8.2.2",
    "multer": "^1.4.5-lts.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
